<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <application>
        <message-bundle>
            messages_es
        </message-bundle>
        
        <resource-bundle>
            <base-name>messages_es</base-name>
            <var>msg</var>
        </resource-bundle>
        
        <locale-config>
            <default-locale>es_MX</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>de</supported-locale>
            <supported-locale>es</supported-locale>
            <supported-locale>fr</supported-locale>
        </locale-config>
    </application>
    
    <!-- Navigation rules -->
    <navigation-rule>
    <from-view-id>/pages/actos/nacimiento/acta/consultaRegistroNacimiento/consultaRegistroNacimiento.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>detalle</from-outcome>
        <to-view-id>/pages/actos/nacimiento/acta/detalleRegistroNacimiento/detalleRegistroNacimiento.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
        <from-outcome>consulta</from-outcome>
        <to-view-id>/pages/actos/nacimiento/acta/detalleRegistroNacimiento/detalleRegistroNacimiento.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
</faces-config>
